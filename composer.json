{
  "name": "robinn/uikit",
  "description": "UI Kit with support for multiple CSS frameworks.",
  "type": "library",
  "keywords": ["templating", "twig", "css", "framework"],
  "license": "MIT",
  "authors": [
    {
      "name": "Róbert Kelčák",
      "homepage": "https://kelcak.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4",
    "ext-dom": "*",
    "twig/twig": "^3",
    "league/plates": "^3.4",
    "latte/latte": "^2.10"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "RobiNN\\UiKit\\": "src/"
    }
  }
}
